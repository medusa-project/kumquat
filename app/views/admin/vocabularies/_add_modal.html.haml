#dl-add-vocabulary-modal.modal.fade{"aria-labelledby": "dl-add-vocabulary-modal-label",
                                    role: "dialog",
                                    tabindex: "-1"}
  .modal-dialog{role: "document"}
    .modal-content
      .modal-header
        %h4#dl-add-vocabulary-modal-label.modal-title Add Vocabulary
        %button.close{"aria-label": "Close",
                      "data-dismiss": "modal",
                      type: "button"}
          %span{"aria-hidden": "true"} Ã—
      .modal-body
        = render partial: 'form', locals: { vocabulary: @vocabulary,
                                            context: :new }
